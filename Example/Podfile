  pod 'RenderCloud', :git => 'git@bitbucket.org:renderapps/RenderCloud.git', :tag=>'2.0.0'
  #pod 'PannaPay', :git => 'git@bitbucket.org:renderapps/pannapay.git', :tag=>'1.0.0'
  pod 'PannaPay', :path => '../../pannapay'
  pod 'Stripe', '14.0.0'
  pod 'RACameraHelper', :git => 'https://github.com/bobbyren/RACameraHelper', :tag => '0.1.5'
  
  # heatmaps
  # https://stackoverflow.com/questions/42611599/cocoapods-target-has-transitive-dependencies-that-include-static-binaries
  pod 'GoogleMaps'

  target 'Balizinha_Tests' do
    inherit! :search_paths

  end
end
